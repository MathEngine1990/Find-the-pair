
  {
  "static_path": "dist",
  "app_id": 53937545,
  "endpoints": {
    "mobile": "/",
    "web": "/", 
    "mvk": "/"
  },
  "alias": {
    "mobile": "findpair-mobile",
    "web": "findpair-web"
  },
  "headers": {
    "Content-Security-Policy": "default-src 'self' 'unsafe-inline' 'unsafe-eval' https://unpkg.com https://cdnjs.cloudflare.com; img-src 'self' data: blob:; connect-src 'self' https://api.vk.com;",
    "X-Frame-Options": "SAMEORIGIN",
    "X-Content-Type-Options": "nosniff"
  },
  "rewrites": [
    {
      "source": "/assets/(.*)",
      "destination": "/assets/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "cors": {
    "allowOrigins": [
      "https://vk.com",
      "https://m.vk.com", 
      "https://vk-apps.com"
    ],
    "allowMethods": ["GET", "POST"],
    "allowHeaders": ["Content-Type", "Authorization"]
  }
}
